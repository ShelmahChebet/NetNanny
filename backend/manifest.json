{
    "manifest_version": 3,
    "name": "Youtube And Email Scraper",
    "version": "1.0",
    "permissions": ["scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": [""],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "action": {
    "default_popup": "popup.html"
    }
  }
  